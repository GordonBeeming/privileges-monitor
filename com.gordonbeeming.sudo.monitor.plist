<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.gordonbeeming.sudo.monitor</string>
    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>/usr/local/bin/privileges-monitor/privileges_sudo_monitor.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true />
    <key>KeepAlive</key>
    <true />
    <key>StandardErrorPath</key>
    <string>/tmp/com.gordonbeeming.sudo.monitor.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.gordonbeeming.sudo.monitor.out</string>
  </dict>
</plist>